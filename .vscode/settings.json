{
  "cSpell.words": [
    "APPDATA",
    "arange",
    "bardapi",
    "dhash",
    "dotenv",
    "dtype",
    "fourcc",
    "fromarray",
    "hconcat",
    "hstack",
    "imagehash",
    "imwrite",
    "lapsquash",
    "lapsquasher",
    "ndarray",
    "oclif",
    "polyfit",
    "pyqtgraph",
    "tbard",
    "TRPC",
    "UDEV",
    "vconcat",
    "YOLO"
  ],
  "python.defaultInterpreterPath": ".\\squasher_core\\.venv\\Scripts\\python.exe",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "files.exclude": {
    "**/__pycache__": true
  },
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingTypeStubs": "none"
  },
  "python.analysis.typeCheckingMode": "strict",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true,
    "source.organizeImports": true
  }
}
