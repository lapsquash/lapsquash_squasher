{
  "cSpell.words": [
    "APPDATA",
    "arange",
    "dtype",
    "fromarray",
    "imagehash",
    "lapsquash",
    "lapsquasher",
    "ndarray",
    "oclif",
    "pyqtgraph",
    "TRPC",
    "UDEV",
    "YOLO"
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/squasher_py/.venv/bin/python",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.provider": "none",

  "files.exclude": {
    "**/__pycache__": true
  },
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingTypeStubs": "none"
  },
  "python.analysis.typeCheckingMode": "strict"
}
