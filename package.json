{
  "name": "lapsquash",
  "description": "lapsquash",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "poetry": "cd squasher_py && poetry",
    "dev": "yarn workspace @lapsquash/squasher run dev",
    "dev:py": "yarn poetry run python ./squasher_py/main.py",
    "dev:analyzer": "yarn workspace @lapsquash/analyzer run dev",
    "test:analyzer": "yarn workspace @lapsquash/analyzer run test",
    "build": "yarn workspace @lapsquash/squasher run build",
    "prepare": "yarn poetry install"
  },
  "workspaces": {
    "packages": [
      "analyzer",
      "squasher"
    ]
  }
}
